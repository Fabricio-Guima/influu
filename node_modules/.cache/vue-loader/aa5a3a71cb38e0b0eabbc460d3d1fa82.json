     </li>\n      </ul>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      cep: \"25585-540\",\n      arrayCep: null,\n      infoCep: {},\n      showInfoCep: false,\n      disabledBtn: false\n    };\n  },\n  methods: {\n    async getCep() {\n      if (this.cep.length > 8) {\n        let dividedCep = this.cep.split(\"-\");\n\n        this.arrayCep = dividedCep[0] + dividedCep[1];\n\n        console.log(this.arrayCep);\n\n        this.disabledBtn = true;\n\n        fetch(`https://viacep.com.br/ws/${parseInt(this.arrayCep)}/json/`)\n          .then((r) => {\n            return r.json();\n          })\n          .then((r) => {\n            console.log(r);\n            this.showInfoCep = true;\n            delete r.cep;\n            delete r.complemento;\n            delete r.gia;\n            delete r.ddd;\n            delete r.siafi;\n\n            var cepObj = {\n              logradouro: r[\"logradouro\"],\n              bairro: r[\"bairro\"],\n              cidade: r[\"localidade\"],\n              estado: r[\"uf\"],\n              ibge: r[\"ibge\"]\n            };\n\n            console.log(cepObj);\n\n            this.infoCep = cepObj;\n            this.disabledBtn = false;\n          })\n          .catch((err) => {\n            console.log(err);\n          });\n      }\n    }\n  }\n};\n</script>\n\n<style scoped>\n.cep {\n  /* background: red; */\n  font-family: Raleway;\n}\n\nh1 {\n  padding-bottom: 1rem;\n  font-family: raleway;\n  font-weight: bold;\n  font-size: 36px;\n  color: #ffffff;\n  margin-top: 24px;\n}\np.sub-text {\n  font-family: raleway;\n  position: relative;\n  margin-bottom: 94px;\n  color: #ffffff;\n}\n\np.sub-text::after {\n  content: \"\";\n  display: block;\n  max-width: 30px;\n  height: 3px;\n  margin-top: 1rem;\n  background-color: #ef328d;\n}\n\n.button-group {\n  display: flex;\n  justify-content: center;\n}\n\n.search-cep {\n  border: 1px solid #944089;\n  border-radius: 4px;\n}\n\ninput {\